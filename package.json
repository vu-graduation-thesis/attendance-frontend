{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:test": "vite build .",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint-fix": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "npx stylelint '**/*.scss'",
    "prettify": "prettier --write .",
    "prettier:check": "prettier --list-different ./src/**/*.{js, jsx,ts,tsx,scss, css}",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "start-android": "npx cap run android"
  },
  "dependencies": {
    "@capacitor-community/camera-preview": "^5.0.0",
    "@capacitor/android": "^5.2.3",
    "@capacitor/core": "^5.2.3",
    "@capacitor/ios": "^5.2.3",
    "antd": "^5.6.1",
    "axios": "^1.4.0",
    "chart.js": "^4.3.3",
    "classnames": "^2.3.2",
    "i18next-browser-languagedetector": "^7.0.2",
    "keycloak-js": "^21.1.1",
    "luxon": "^3.4.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.12.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.2.3",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/luxon": "^3.3.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "stylelint": "^15.7.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run lint-fix",
      "npm run prettify",
      "git add ."
    ],
    "./src/*.{css, scss}": [
      "npm run lint:css"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
